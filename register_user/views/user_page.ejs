  <script type="text/javascript">
    $(function() {
      $(".start-service").click(function(){
      var id=this.id
       bootbox.dialog({
        message: "Are you sure start "+id+" ?",
        title: "Start Apps",
        buttons: {
          success: {
            label: "Cancel",
            className: "btn-info",
            callback: function() {
              
            }
          },
          buttons: {
            label: "Start",
            className: "btn-complete",
            callback: function() {
              console.log(id)
            }
          }
        }
      });
    })

    $(".stop-service").click(function(){
      var id=this.id
       bootbox.dialog({
        message: "Are you sure stop "+id+" ?",
        title: "Stop Apps",
        buttons: {
          success: {
            label: "Cancel",
            className: "btn-info",
            callback: function() {
              
            }
          },
          buttons: {
            label: "Stop",
            className: "btn-primary",
            callback: function() {
              console.log(id)
            }
          }
        }
      });
    });
    $(".delete-service").click(function(){
       var id=this.id
       bootbox.dialog({
        message: "Are you sure delete "+id+" ?",
        title: "Delete Apps",
        buttons: {
          success: {
            label: "Cancel",
            className: "btn-info",
            callback: function() {
              
            }
          },
          buttons: {
            label: "Delete",
            className: "btn-danger",
            callback: function() {
              console.log(id)
            }
          }
        }
      }); 
    });
    $( "#slider-range-min" ).slider({
      range: "min",
      value: 1,
      min: 0,
      max: 1000,
      slide: function( event, ui ) {
        $( "#amount" ).val( "IDR " +ui.value *3+ "000");
        $( "#loot" ).val( ui.value +" Loots" );
        $( "#memory" ).val( ui.value  + " MB");
        $( "#space" ).val( (ui.value * 0.03).toFixed(3) + " GB");
        $( "#bandwith" ).val( ui.value  + " GB");
        $( "#request" ).val( ui.value *1000 + " times/month");
        if(ui.value > 500)$( "#worker" ).val("2 worker");
        else $( "#worker" ).val("1 worker");
        $(".a, .b, .c, .d").width(ui.value/10 + "%");
      }
    });
    $(".ui-slider-handle").text("<>");
    $( "#amount" ).val( "IDR " + $( "#slider-range-min" ).slider( "value")*3 + "000");
    $( "#loot" ).val( $( "#slider-range-min" ).slider( "value") +" Loots");
    $( "#memory" ).val(  $( "#slider-range-min" ).slider( "value") + " MB");
    $( "#space" ).val( $( "#slider-range-min" ).slider( "value")*0.03 + " GB");
    $( "#bandwith" ).val( $( "#slider-range-min" ).slider( "value") + " GB");
    $( "#request" ).val( $( "#slider-range-min" ).slider( "value")*1000 + " times/month");
    $( "#worker" ).val("1 worker");
  });
  $('#submitNewService').submit(function(){
      var amount=$( "#amount" ).val();
      var serviceName=$( "#serviceName" ).val();
      var loot=$( "#loot" ).val();
      var memory=$( "#memory" ).val();
      var space=$( "#space" ).val();
      var bandwith=$( "#bandwith" ).val();
      var request=$( "#request" ).val();
      var worker=$( "#worker" ).val();
      var newApp=$( "#newApp" ).val();
      var newDb=$( "#newDb" ).val();
      var data={
        amount:amount,
        serviceName:serviceName,
        loot:loot,
        memory:memory,
        space:space,
        bandwith:bandwith,
        request:request,
        worker:worker,
        newApp:newApp,
        newDb:newDb
      }
      //console.log(amount+" "+loot+" "+memory+" "+space+" "+bandwith+" "+request+" "+worker+" "+newDb+" "+newApp + " "+serviceName);
      $.post("/newService", {data:data}, function(result){
        if(result.status=="OK")
        {
            bootbox.dialog({
              message: "Service successfully created",
              title: "Create new service",
              buttons: {
                success: {
                  label: "OK",
                  className: "btn-success",
                  callback: function() {
                    window.location='/userpage';
                  }
                }
              }
            });

        }
        else
        {
          bootbox.dialog({
              message: "Failed create new service, duplicate name",
              title: "Create new service",
              buttons: {
                success: {
                  label: "OK",
                  className: "btn-danger",
                  callback: function() {
                    
                  }
                }
              }
            });
        }
        console.log(result)
      });
      return false; // avoid to execute the actual submit of the form.
  });
  $('#loot').change(function(){
      $( "#amount" ).val("IDR "+ $('#loot').val() *3+ "000");
      $( "#memory" ).val( $('#loot').val().split(" ")[0]  + "MB");
      $( "#space" ).val( ($('#loot').val() * 0.03).toFixed(3) + " GB");
      $( "#bandwith" ).val( $('#loot').val()  + "GB");
      $( "#request" ).val( $('#loot').val() *1000 + " times/month");
      $(".a, .b, .c, .d").width($('#loot').val()/10 + "%");
  });

      </script>